@code {
private string GetCanonicalUrl()
{
    var uri = new Uri(NavigationManager.Uri);
    var canonicalUri = new UriBuilder(uri)
    {
    Scheme = "https",
    Host = "ms-estates.com",
    Port = -1 
    };
return canonicalUri.ToString();
}
}